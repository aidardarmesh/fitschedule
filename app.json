{
  "expo": {
    "name": "FitSchedule",
    "slug": "expo-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1766657798756.png",
    "scheme": "expo-project",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adarmesh.expoproject",
      "icon": "./assets/icon.icon",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "Importing contacts will automatically onboard members into your schedule."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.adarmesh.expoproject"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9152539e-7be5-44d3-9389-767611765a9c"
      }
    },
    "owner": "adarmesh",
    "updates": {
      "url": "https://u.expo.dev/9152539e-7be5-44d3-9389-767611765a9c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}